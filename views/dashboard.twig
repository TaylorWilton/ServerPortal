<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport"
        content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="./assets/main.css">
  <link href="https://fonts.googleapis.com/css?family=Ubuntu|Ubuntu+Condensed" rel="stylesheet">
  <title>Aincrad</title>
</head>
<body>
  <div class="container" id="app">
    <aside id="aincrad">
    </aside>
    <div class="content-main">
      <h1>Welcome to Aincrad!</h1>
      <div class="grid">

        <div class="inner-grid">
          {% include 'partials/hackernews.twig' %}
          {% include'partials/rss.twig' with {id: "bbc-rss"} %}
        </div>

        <div class="inner-grid">
          {% include 'partials/weather.twig' %}
          {% include 'partials/status.twig' %}
          {% include 'partials/diskUsage.twig' %}
        </div>
      </div>
      {% include 'partials/anime.twig' %}
    </div>
  </div>
  <script src="./assets/vue.js"></script>
  <script src="./assets/main.js"></script>
</body>
</html>