<div class="block">
  <h2>Top HackerNews Posts</h2>
  <div class="inner" id="hackernews">
  <div class="loader" v-if="loading"></div>
  <ol v-if="!loading">
    <li v-for="(post,key) in posts"
        v-if="key !== 'cachedAt'">
      <a :href="post.url">
        ${ post.title }
      </a>
      <small class="comments-link"><a :href="'https://news.ycombinator.com/item?id=' + post.id">Comments</a></small>
    </li>
  </ol>
  </div>
</div>